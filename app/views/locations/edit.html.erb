<h1>Edit your Location</h1>

<%= simple_form_for @location do |f| %>
       <%= f.input :title %>
       <%= f.input :address %>
       <%= f.input :description %>
       <%= f.input :size %>
       <%= f.input :price%>
       <%= f.input :tags%>
       <%= f.button :submit, class: 'btn btn-warning' %>
     <% end %>
